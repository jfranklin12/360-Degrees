<div class="container">
       
    <div class="row">
        <div class="col s12">
         {{#each review as |reviews|}}
            <div class="card blue-grey darken-1">
                <div class="card-content white-text">
                    <span class="card-title"></span>
                    <div class="card-action">
                        <a>{{score_1}}</a>
                        <a>{{score_2}}</a>
                        <a>{{score_3}}</a>
                        <a>{{score_4}}</a>
                    </div>
                <p>User Comment</p>
        </div>
        {{/each}}
    </div>

</div>
   
