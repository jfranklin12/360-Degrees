<div class="container">
    {{#each reviews as |review|}}
    <div class="row">
        <div class="col s12">
            <div class="card blue-grey darken-1">
                <div class="card-content white-text">
                    <span class="card-title">Degree Name</span>
                    <div class="card-action">
                        <a>{{review.score_1}}</a>
                        <a>{{review.score_2}}</a>
                        <a>{{review.score_3}}</a>
                        <a>{{review.score_4}}</a>
                    </div>
                    <p>{{review.comment}}</p>

                </div>
            </div>
        </div>
    </div>
    {{/each}}
</div>
